{
  "name": "moltbot-scan",
  "version": "0.2.0",
  "description": "Moltbook Agent Trust Scanner SDK - Detect prompt injection, credential theft, and social engineering in agent messages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "default": "./dist/middleware/index.js"
    },
    "./analyzers": {
      "types": "./dist/analyzers/index.d.ts",
      "default": "./dist/analyzers/index.js"
    }
  },
  "bin": {
    "agentshield": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "serve": "ts-node src/web/server.ts",
    "batch-scan": "tsc && node dist/data/batch-scan.js",
    "test": "jest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "moltbook",
    "agent",
    "trust",
    "security",
    "scanner",
    "prompt-injection",
    "ai-safety",
    "middleware",
    "sdk"
  ],
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "express": "^4.18.0",
    "node-fetch": "^3.3.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.39.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "parquetjs-lite": "^0.8.7",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
